		tr {
		th 'header 1'
		td 'row 1, element 1'
		td 'row 1, element 2'
		}
			
		tr {
			th 'header 2'
			td 'row 2, element 1'
			td 'row 2, element 2'
		}
	}
	
	BulletPoint {
		p 'bullet point element 1'
		h6 'bullet point element 2'
	}
	
	NumberedList {
		p 'numbered list element 1'
		h6 'numbered list element 2'
	}
	
	DropDownSelection {
		label 'choose an element'
		options {
			'option 1'
			'option 2'
			'option 3'
		}
	}
}

